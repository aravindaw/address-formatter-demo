<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Omnius test floors" parallel="tests" thread-count="1" verbose="1">
    <!--sample browser parameter types (chrome, firefox and ie)-->
    <test name="TC_01">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.aravinda.app.testCase.TC_01_AccessAddressFormatterPage"/>
        </classes>
    </test>
    <test name="TC_02">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.aravinda.app.testCase.TC_02_FormatAnAddress"/>
        </classes>
    </test>
    <test name="TC_03">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.aravinda.app.testCase.TC_03_ResetAddressField"/>
        </classes>
    </test>
    <test name="TC_04">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.aravinda.app.testCase.TC_04_InvalidAddressFormat"/>
        </classes>
    </test>
    <test name="TC_05">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.aravinda.app.testCase.TC_05_TestListOfAddresses"/>
        </classes>
    </test>
    <test name="TC_06">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.aravinda.app.testCase.TC_06_InvalidAddressFormat_IntOnly"/>
        </classes>
    </test>
</suite>